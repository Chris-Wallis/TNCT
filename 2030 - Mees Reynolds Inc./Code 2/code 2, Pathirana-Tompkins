campaignTrail_temp.candidate_image_url = "https://i.postimg.cc/y8XscrNT/Santhush.png";
campaignTrail_temp.running_mate_image_url = "https://i.postimg.cc/WzdsZMWY/Joel.png";
campaignTrail_temp.candidate_last_name = "Pathirana";
campaignTrail_temp.running_mate_last_name = "Tompkins";

campaignTrail_temp.multiple_endings = true;
endingPicker = (out, totv, aa, quickstats) => {

    function setImage(url) {
        if(url == '' || url == null) return;
        let interval = setInterval(function () {
            img = document.getElementsByClassName("person_image")[0];
            if (img != null) {
                img.src = url;
                clearInterval(interval);
            }
        }, 50);
    }

    if(quickstats[0] > 270) {
        setImage("https://i.postimg.cc/cL7JxJy5/Daniel.png");
        return `<h3> Breaking news! Daniel Rogers' Progressive Party elected in majority!<h3><p>Whilst you were the favourite to win re-election, it is still a happy moment for you to see all your hard work pay off. Unless you've done super well, you still have a hostile House and Senate to deal with, and the Liberal Socialists will try to force socialism upon you at any cost. Mees and Wallis have both said they won't recontest, and Pathirana looks the strong favourite for 2034. Anyways, good luck for your second term!
</p>`;
    }
    if(quickstats[0] < 100) {
        setImage("https://i.postimg.cc/8kJGtC5J/Oli.png");
        return `<h3>Sorry! You have lost the 2026 election.</h3><p>Oliver Mees has won a majority of the electoral college. Prepare for four years of moderate, independent rule. You can still likely block him in the house, with the help of the Liberal Socialists. Santhush Pathirana will most likely run in 2034, however Charlie Aston is also looking to run.</p>`;
    }
    if(quickstats[0] > 100) {
        setImage("https://i.postimg.cc/fykjJ4vr/brennan-and-wallis.jpg");
        return `<h3>No candidate has a majority.</h3><p>Luckily for you, Wallis and the Liberal Socialists will help you form government. To gain the support of the Liberal Socialists, you will need to make Wallis VP, and make strong economic moves, or risk losing their support in the House. Good luck, you'll need it to deal with them.</p>`;
    }
}


